<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="chrome://beef-taco/skin/overlay.css" type="text/css"?>
<!DOCTYPE overlay SYSTEM "chrome://beef-taco/locale/overlay.dtd">
<overlay id="beef-taco-overlay" xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">
  <script src="namespace.js"/>
  <script src="ff-overlay.js"/>

  <stringbundleset id="stringbundleset">
    <stringbundle id="beef-taco-strings" src="chrome://beef-taco/locale/overlay.properties"/>
  </stringbundleset>

  <menupopup id="menu_ToolsPopup">
    <menu id="beef-taco-menu" label="&beef-taco.label;" class="menu-iconic" image="chrome://beef-taco/content/icon.16x16.png">
      <menupopup id="beef-taco-menu-popup">
        <menuitem label="&beef-taco.viewCookiesLabel;" oncommand="com.github.jmhobbs.beef_taco.showAllCookies()" class="menuitem-iconic" image="chrome://beef-taco/content/cookie.16x16.png" />
      </menupopup>
    </menu>
  </menupopup>
</overlay>